{
  "compilerOptions": {
    "experimentalDecorators": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "target": "es5",
    "module": "es2015",
    "jsx": "react",
    "lib": [
      "dom",
      "es2015.core",
      "es2015.promise",
      "es2015.symbol",
      "es2015.collection",
      "es5"
    ],
    "baseUrl": "",
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
